word = "иинформаця"
correct_word = "информация"
word_list = list(word)
correct_word_list = list(correct_word)
for i in range(len(word_list)):
  if word_list[i] in correct_word_list:
    correct_word_list.remove(word_list[i])
    word_list[i] = correct_word[i]
    corrected_word = "".join(word_list)

  print(corrected_word)
